    "adaptivecards": "3.0.4",
    "adaptivecards-templating": "2.3.1",
    "botbuilder": "4.23.0",
    "botframework-connector": "4.23.0",
    "botframework-schema": "4.22.3",
    "restify": "11.1.0"
  },
  "devDependencies": {
    "@types/node": "20.14.10",
    "@types/restify": "8.5.5",
    "env-cmd": "10.1.0",
    "gts": "5.3.1",
    "nodemon": "3.1.4",
    "shx": "^0.3.3",
    "ts-node": "10.4.0",
    "typescript": "5.5.4"
  }
